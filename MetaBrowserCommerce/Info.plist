			<key>CFBundleURLSchemes</key>
			<array>
				<string>metabrowsercommerce</string>
			</array>
			<key>CFBundleURLName</key>
			<string>com.omnia.MetaBrowserCommerce</string>
		</dict>
	</array>
	<key>MWDAT</key>
	<dict>
		<key>Analytics</key>
		<dict>
			<key>OptOut</key>
			<true/>
		</dict>
